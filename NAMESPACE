## IMPORT METHODS FROM STATS
importFrom(stats, predict)

## EXPORT MAIN bfrm FUNCTION
exportMethods(
  bfrm
)

## EXPORT CLASSES
exportClasses(
  bfrmModel,
  bfrmLinearModel,
  bfrmBinaryModel,
  bfrmSurvivalModel,
  bfrmCategoricalModel,
  bfrmResult,
  bfrmLinearResult,
  bfrmBinaryResult,
  bfrmSurvivalResult,
  bfrmCategoricalResult
)
